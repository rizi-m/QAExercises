package paint;

public class Paint {

	private String name;
	private int amount;
	private float price;
	private int areaPerLitre;
	
	public Paint(String name, int amount, float price, int areaPerLitre) {
		super();
		this.name = name;
		this.amount = amount;
		this.price = price;
		this.areaPerLitre = areaPerLitre;
	}

	public String getName() {
		return name;
	}

	public float getPrice() {
		return price;
	}

	public int getAreaPerLitre() {
		return areaPerLitre;
	}
	
	public int getAreaCovered() {
		
	}
	

}
